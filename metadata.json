{
  "icon": "localstack.svg",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "extension-LocalStack.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "LocalStack",
      "src": "index.html",
      "root": "ui",
      "backend": {
        "socket": "extension-LocalStack.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/checkUser.sh"
          }
        ],
        "linux": [
          {
            "path": "/linux/checkUser.sh"
          }
        ],
        "windows": [
          {
            "path": "/windows/checkWSLOS.cmd"
          },
          {
            "path": "/windows/checkUser.cmd"
          }
        ]
      }
    ]
  }
}
